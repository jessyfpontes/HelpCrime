USE db_help;
CREATE TABLE `sprint` (
 `nome` int(11) UNSIGNED AUTO_INCREMENT PRIMARY KEY,
  `email` varchar(50) COLLATE utf8_unicode_ci NOT NULL,
   `number` varchar(1) COLLATE utf8_unicode_ci NOT NULL,
    `message` varchar(1) COLLATE utf8_unicode_ci NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=UTF8_UNICODE_CI;
